%body
  %header.pcHeader
    .headerInner
      .mainHeader
        %h1.icon
          %a{href: "/"}
            %img{src: "/assets/logo.png"}
        .searchBox
          %form{"accept-charset" => "UTF-8", action: "#", method: "get"}
            %input{name: "utf8", type: "hidden", value: "✓"}
            %input#searchInput.searchBox--input{name: "key", placeholder: "キーワードから探す"}/
            %button{type: "submit"}
              .searchBox__icon
                %img{src: "/assets/icon-search 1.png"}
      %ul.resultLists
      .nav
        %ul.listsLeft
          %li.listsLeft__item.listsLeft__item--first
            %a#catBtn{href: "#"} カテゴリー
            .tree.displayNone
              %ul.categoryTree
                %li.categoryTree--item
                  %a#1{href: "#"} レディース
                %li.categoryTree--item
                  %a#2{href: "#"} メンズ
                %li.categoryTree--item
                  %a#3{href: "#"} ベビー・キッズ
