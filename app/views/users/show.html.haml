                  %a#5{href: "#"} 本・音楽・ゲーム
                %li.categoryTree--item
                  %a#6{href: "#"} おもちゃ・ホビー・グッズ
                %li.categoryTree--item
                  %a#7{href: "#"} コスメ・香水・美容
                %li.categoryTree--item
                  %a#8{href: "#"} 家電・スマホ・カメラ
                %li.categoryTree--item
                  %a#9{href: "#"} スポーツ・レジャー
                %li.categoryTree--item
                  %a#10{href: "#"} ハンドメイド
                %li.categoryTree--item
                  %a#11{href: "#"} チケット
                %li.categoryTree--item
                  %a#12{href: "#"} 自動車・オートバイ
                %li.categoryTree--item
                  %a#13{href: "#"} その他
          %li.listsLeft__item
            %a#brandBtn{href: "#"} ブランド
            %ul.brandsPulldown.displayNone
        %ul.listsRight
          %li.listsRight__item.listsRight__item--login
            %a{href: "#"} ログアウト
          %li.listsRight__item.listsRight__item--new
            %a{href: "#"} 新規会員登録
